<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projecttechapp.Win2Controller">
   <children>
      <AnchorPane id="AnchorPane" layoutX="3.0" layoutY="3.0" prefHeight="392.0" prefWidth="581.0">
         <children>
            <Label layoutX="67.0" layoutY="53.0" text="Choose Event" textFill="#374171">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <ImageView fx:id="LeapEvents" fitHeight="150.0" fitWidth="200.0" layoutX="214.0" layoutY="125.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Leap.jpg" />
               </image>
            </ImageView>
            <Button fx:id="LeapReg" layoutX="226.0" layoutY="231.0" mnemonicParsing="false" onAction="#leap_reg" prefHeight="25.0" prefWidth="74.0" style="-fx-background-color: #88DAFE;" text="Register " />
            <ImageView fx:id="atHackEvents" fitHeight="150.0" fitWidth="200.0" layoutX="48.0" layoutY="125.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../blackHat.jpg" />
               </image>
            </ImageView>
            <Button fx:id="atHackReg" layoutX="86.0" layoutY="245.0" mnemonicParsing="false" onAction="#atHack_reg" prefHeight="25.0" prefWidth="67.0" style="-fx-background-color: #88DAFE;" text="Register" />
            <ImageView fx:id="AiEvents" fitHeight="150.0" fitWidth="200.0" layoutX="397.0" layoutY="125.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Ai.jpg" />
               </image>
            </ImageView>
            <Button fx:id="AiReg" layoutX="466.0" layoutY="231.0" mnemonicParsing="false" onAction="#Ai_reg" style="-fx-background-color: #88DAFE;" text="Register" />
            <Button fx:id="back" layoutX="44.0" layoutY="321.0" mnemonicParsing="false" onAction="#backReg" prefHeight="25.0" prefWidth="50.0" style="-fx-background-color: #374171;" text="Back" textFill="WHITE" />
            <ImageView fitHeight="125.0" fitWidth="129.0" layoutX="466.0" layoutY="296.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../c.jpg" />
               </image>
            </ImageView>
            <ImageView fitHeight="57.0" fitWidth="74.0" layoutX="-7.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../blue.jpg" />
               </image>
            </ImageView>
            <MenuBar layoutX="-4.0" prefHeight="25.0" prefWidth="146.0">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem fx:id="closeMenu" mnemonicParsing="false" onAction="#close" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem fx:id="aboutt" mnemonicParsing="false" onAction="#aboutInfor" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
